{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "sneezier",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.sneezier",
				"match": "\\b(C|Q|M)\\b"
			}, {
				"name": "keyword.control.sneezier2",
				"match": "\\[.*\\]"
			}]
		}
	},
	"scopeName": "source.snz"
}