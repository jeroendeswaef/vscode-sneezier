{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "sneezier",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "variable.name",
				"match": "\\b(C|Q|M|L|A)\\b"
			}, {
				"name": "keyword.control.sneezier2",
				"match": "\\[.*\\]"
			}, {
				"name": "constant.character",
				"match": "\\b(CIRCLE)\\b"
			}, {
				"name": "comment.line",
				"match": "\\#.*"
			}]
		}
	},
	"scopeName": "source.snz"
}